<?xml version="1.0" encoding="utf-8" ?>
<FileRecognizerList>

    <FileRecognizer name="CV2">
        <RegexMatcher>.*CV2.*\.DTA</RegexMatcher>
        <ExtractableElements>
            <RegexExpression>([0-9]+)\s([A-Z]+)\s([1-9][0-9]+[n|u|m]M)\s([1-9]+)\s([A-F]+)\s([0-9]+)\s+CV2_#([0-9]+).DTA</RegexExpression>
            <Element name="Date" group="1"/>
            <Element name="Chem" group="2"/>
            <Element name="Concentration" group="3"/>
            <Element name="Trial" group="4"/>
            <Element name="Scientist" group="5"/>
            <Element name="IDK" group="6"/>
            <Element name="fileno" group="7"/>
        </ExtractableElements>
    </FileRecognizer>

